{
  "name": "@aibtcdev/tools",
  "version": "0.0.3",
  "description": "A TypeScript SDK for interacting with DAOs and their extensions on the Stacks blockchain",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "bun run clean && bun build ./src/stacks-dao/index.ts --outdir ./dist --target node",
    "build:types": "tsc --project ./src/stacks-dao/tsconfig.json --declaration --emitDeclarationOnly --outDir dist",
    "prepare": "bun run build && bun run build:types",
    "prepublishOnly": "bun run test -t dao",
    "test": "bun test src/stacks-dao/index.test.ts",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@bitflowlabs/core-sdk": "^1.0.1",
    "@jingcash/core-sdk": "^0.1.4",
    "@stacks/blockchain-api-client": "^8.4.0",
    "@stacks/bns": "^6.15.0",
    "@stacks/connect": "^7.7.1",
    "@stacks/stacking": "^6.16.1",
    "@stacks/stacks-blockchain-api-types": "^7.9.0",
    "@stacks/transactions": "^6.12.1",
    "@stacks/wallet-sdk": "^6.12.1",
    "bitflow-sdk": "^1.6.1",
    "eta": "^3.5.0",
    "stxcity-sdk": "^1.0.10",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "@types/node": "^22.10.2",
    "bun-types": "^1.1.17"
  },
  "keywords": [
    "stacks",
    "blockchain",
    "dao",
    "smart-contracts",
    "typescript",
    "sdk"
  ],
  "author": "aibtcdev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aibtcdev/agent-tools-ts.git"
  },
  "bugs": {
    "url": "https://github.com/aibtcdev/agent-tools-ts/issues"
  },
  "homepage": "https://github.com/aibtcdev/agent-tools-ts#readme",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
